
---
import Base from '@/layouts/Base.astro';
import { getCollection } from 'astro:content';
import SourceRow from '@/components/SourceRow.astro';
const items = await getCollection('library');
---
<Base title="Library">
  <h1 class="text-2xl font-semibold mb-6 tracking-widest">/library</h1>
  <ul class="divide-y divide-zinc-800">
    {items.map((it) => <SourceRow item={it} />)}
  </ul>
</Base>
